{% extends 'base.html.twig' %}

{% block title %}Ajouter une demande d'enseignement{% endblock %}

{% block body %}
    <h1>Ajouter une demande d'enseignement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ demande_enseignement.id }}</td>
            </tr>
            <tr>
                <th>NomDemande</th>
                <td>{{ demande_enseignement.nomDemande }}</td>
            </tr>
            <tr>
                <th>DescriptionDemande</th>
                <td>{{ demande_enseignement.descriptionDemande }}</td>
            </tr>
            <tr>
                <th>Decision</th>
                <td>{{ demande_enseignement.decision ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>UrlParticipation</th>
                <td>{{ demande_enseignement.urlParticipation }}</td>
            </tr>
        </tbody>
    </table>
<button type="submit">

    <a href="{{ path('demande_enseignement_index') }}">Afficher la liste des demandes </a>
</button>
<button type="submit">

<a href="{{ path('demande_enseignement_edit', {'id': demande_enseignement.id}) }}">Modifier Votre demande </a>
</button>
    {{ include('demande_enseignement/_delete_form.html.twig') }}
{% endblock %}
