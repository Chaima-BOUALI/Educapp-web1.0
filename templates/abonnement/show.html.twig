{% extends 'base.html.twig' %}

{% block title %}Abonnement{% endblock %}

{% block body %}
    <h1>Abonnement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ abonnement.id }}</td>
            </tr>
            <tr>
                <th>TypeAbonnement</th>
                <td>{{ abonnement.typeAbonnement }}</td>
            </tr>
            <tr>
                <th>DateAbonnement</th>
                <td>{{ abonnement.dateAbonnement ? abonnement.dateAbonnement|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('abonnement_index') }}">back to list</a>
<button type="submit">
    <a href="{{ path('abonnement_edit', {'id': abonnement.id}) }}">Modifier</a>
</button>
<button type="submit">
    {{ include('abonnement/_delete_form.html.twig') }}
</button>
{% endblock %}
